{
  "version": "1",
  "groups": {
    "Motivation": [
      "Assessment", "Constraint", "Driver", "Goal", "Meaning",
      "Outcome", "Principle", "Requirement", "Stakeholder", "Value"
    ],
    "ActiveStructure": [
      "BusinessActor", "BusinessRole", "BusinessCollaboration", "BusinessInterface",
      "ApplicationComponent", "ApplicationCollaboration", "ApplicationInterface",
      "CommunicationNetwork", "Device", "Node", "SystemSoftware",
      "TechnologyCollaboration", "TechnologyInterface",
      "Equipment", "Facility", "DistributionNetwork", "Path"
    ],
    "Behavior": [
      "BusinessProcess", "BusinessFunction", "BusinessInteraction",
      "ApplicationProcess", "ApplicationFunction", "ApplicationInteraction",
      "TechnologyProcess", "TechnologyFunction", "TechnologyInteraction",
      "BusinessEvent", "ApplicationEvent", "TechnologyEvent",
      "Capability", "CourseOfAction", "ValueStream"
    ],
    "Service": [
      "BusinessService", "ApplicationService", "TechnologyService"
    ],
    "PassiveStructure": [
      "BusinessObject", "Contract", "Representation", "Product",
      "DataObject", "Artifact", "Material", "Deliverable", "Resource"
    ],
    "Composite": [
      "Grouping", "Location"
    ]
  },
  "rules": [
    // General
    { "relationship": "AssociationRelationship",   "sourceGroup": "*",                "targetGroup": "*" },
    { "relationship": "SpecializationRelationship", "sourceGroup": "*",                "targetGroup": "*", "sameClass": true },

    // Structural aggregation/composition
    { "relationship": "AggregationRelationship",   "sourceGroup": "Composite",         "targetGroup": "*" },
    { "relationship": "AggregationRelationship",   "sourceGroup": "ActiveStructure",   "targetGroup": "ActiveStructure" },
    { "relationship": "AggregationRelationship",   "sourceGroup": "ActiveStructure",   "targetGroup": "PassiveStructure" },
    { "relationship": "AggregationRelationship",   "sourceGroup": "PassiveStructure",  "targetGroup": "PassiveStructure" },

    { "relationship": "CompositionRelationship",   "sourceGroup": "Composite",         "targetGroup": "*" },
    { "relationship": "CompositionRelationship",   "sourceGroup": "ActiveStructure",   "targetGroup": "ActiveStructure" },
    { "relationship": "CompositionRelationship",   "sourceGroup": "ActiveStructure",   "targetGroup": "PassiveStructure" },
    { "relationship": "CompositionRelationship",   "sourceGroup": "PassiveStructure",  "targetGroup": "PassiveStructure" },

    // Assignment (assigning behavior to active structure)
    { "relationship": "AssignmentRelationship",     "sourceGroup": "ActiveStructure",   "targetGroup": "Behavior" },

    // Realization
    { "relationship": "RealizationRelationship",   "sourceGroup": "Behavior",         "targetGroup": "Service" },
    { "relationship": "RealizationRelationship",   "sourceGroup": "ActiveStructure",   "targetGroup": "Service" },
    { "relationship": "RealizationRelationship",   "sourceClass": "Artifact",          "targetClass": "DataObject" },
    { "relationship": "RealizationRelationship",   "sourceClass": "Representation",     "targetClass": "BusinessObject" },
    { "relationship": "RealizationRelationship",   "sourceClass": "Deliverable",        "targetGroup": "Motivation" },

    // Serving
    { "relationship": "ServingRelationship",        "sourceGroup": "Service",          "targetGroup": "*" },

    // Access (to passive structure)
    { "relationship": "AccessRelationship",         "sourceGroup": "ActiveStructure",   "targetGroup": "PassiveStructure" },
    { "relationship": "AccessRelationship",         "sourceGroup": "Behavior",         "targetGroup": "PassiveStructure" },

    // Influence (motivation to motivation)
    { "relationship": "InfluenceRelationship",      "sourceGroup": "Motivation",        "targetGroup": "Motivation" },

    // Dynamic relationships
    { "relationship": "TriggeringRelationship",     "sourceGroup": "Behavior",         "targetGroup": "Behavior" },
    { "relationship": "FlowRelationship",           "sourceGroup": "Behavior",         "targetGroup": "Behavior" }
  ]
}

